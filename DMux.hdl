/** if sel=0 return {a,b}={in,0}, else return {a,b}={0,in} */

CHIP DMux { 
    IN in, sel; 
    OUT a, b; 

    PARTS: 
    Nand(a=sel, b=sel, out=c0); //invert sel
    Nand(a=in, b=c0, out=c1); 
    Nand(a=c1, b=c0, out=a); 
    Nand(a=sel, b=in, out=c2); 
    Nand(a=c2, b=c2, out=b); 
}
