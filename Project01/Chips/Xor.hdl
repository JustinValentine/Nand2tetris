/** returns 1 if exactly one of {a, b} is a 1 */

CHIP Xor { 
    
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=a, out=c0); // inverts a 
    Nand(a=b, b=b, out=c1); // inverts b
    Nand(a=a, b=c1, out=c2); 
    Nand(a=c0, b=b, out=c3); 
    Nand(a=c2, b=c3, out=out); 
}
    
